{
  "$schema": "http://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ]
    }
  }
}